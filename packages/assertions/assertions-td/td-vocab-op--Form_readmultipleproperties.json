{
    "title": "td-vocab-op--Form_readmultipleproperties",
    "description": "When a forms term member is present in a Thing instance, the value(s) of op in the forms MUST be one of readmultipleproperties.",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "is-complex": true,
    "also":["well-known-operation-types-only"],
    "properties": {
        "forms": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "#/definitions/form_element_root"
            }
        }
    },
    "additionalProperties": true,
    "definitions": {
        "form_element_root": {
            "if": {
                "type": "object",
                "properties": {
                    "href": {
                        "$ref": "#/definitions/url"
                    },
                    "op": {
                        "oneOf": [{
                                "type": "string",
                                "enum": [
                                    "readmultipleproperties"
                                ]
                            },
                            {
                                "type": "array",
                                "contains": {
                                    "type": "string",
                                    "enum": [
                                        "readmultipleproperties"
                                    ]
                                }
                            }
                        ]
                    }
                },
                "required": [
                    "href", "op"
                ]
            },
            "then": {
                "const": "td-vocab-op--Form_readmultipleproperties=pass"
            }
        },
        "url": {
            "type": "string"
        }
    }
}