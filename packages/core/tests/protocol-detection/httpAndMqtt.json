{
    "id": "urn:simple",
    "@context":"https://www.w3.org/2022/wot/td/v1.1",
    "title": "MyLampThing",
    "description":"Valid TD copied from the spec's first example",
    "securityDefinitions": {
        "basic_sc": {"scheme": "basic","in":"header"}
    },
    "security": ["basic_sc"],
    "properties": {
        "status" : {
            "type": "string",
            "forms": [{"href": "http://mylamp.example.com/status"}]
        }
    },
    "actions": {
        "toggle" : {
            "forms": [{"href": "mqtt://mylamp.example.com/toggle"}]
        }
    },
    "protocolSchemes": ["http", "mqtt"]
}