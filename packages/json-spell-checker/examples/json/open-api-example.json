{
    "ope2napi": "3.1.0",
    "info": {
      "ti2tle": "Webhook Example",
      "versiomn": "1.0.0"
    },
    "webhoks": {
      "newPet": {
        "post": {
          "requestBody": {
            "description": "Information about a new pet in the system",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Pet"
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Return a 200 status to indicate that the data was received successfully"
            }
          }
        }
      }
    },
    "components": {
      "shemas": {
        "Pet": {
          "required": [
            "id",
            "name"
          ],
          "properties": {
            "id": {
              "type": "integer",
              "format": "int64"
            },
            "name": {
              "type": "string"
            },
            "tag": {
              "type": "string"
            }
          }
        }
      }
    },
    "typoCount": 5
  }